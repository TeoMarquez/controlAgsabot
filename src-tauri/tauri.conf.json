{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "controlaggsabot",
  "version": "0.1.0",
  "identifier": "com.controlaggsabot.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "controlAGGSABOT",
        "width": 1200,
        "height": 800,
        "resizable": false,
        "center": true,
        "fullscreen": false
      },
      {
        "title": "Ventana de Trafico UCI",
        "url": "traficoUCI.html",
        "label": "ventana_trafico_uci",
        "visible": false,
        "decorations": true,
        "transparent": false,
        "skipTaskbar": false,
        "alwaysOnTop": false
      },
      {
        "title": "Ventana de Visualizacion 3D",
        "url": "visualizador3D.html",
        "label": "ventana_visualizacion_3d",
        "visible": false,
        "decorations": true,
        "transparent": false,
        "skipTaskbar": false,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../icons/culiao.ico"
    ]
  }
}